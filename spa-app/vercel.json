{
  "rewrites": [
    {
      "source":  "/product/:id",
      "destination":  "https://pwa-open-graph-next.vercel.app/product/:id",
      "has": [
        {
          "type": "host",
          "value": "pwa-open-graph-spa.vercel.app"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source":  "/product/:id",
      "destination":  "https://pwa-open-graph-next.vercel.app/product/:id",
      "missing": [
        {
          "type": "cookie",
          "key": "visited",
          "value": "1"
        }
      ]
    }
  ]
}
{
  "rewrites": [
    {
      "source":  "/:path*",
      "destination":  "https://bakery-og.a16n.dev/:path*",
      "has": [
        {
          "type": "host",
          "value": "bakery.a16n.dev"
        }
      ],
      "missing": [
        {
          "type": "cookie",
          "key": "visited",
          "value": "1"
        },
        {
          "type": "header",
          "key": "Referer",
          "value": "*"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
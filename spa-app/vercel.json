{
  "rewrites": [
    {
      "source":  "/product/:id",
      "destination":  "https://pwa-open-graph-next.vercel.app/product/:id"
    },
    { "source": "/(.*)", "destination": "/index.html",
      "missing": [
        {
        "type": "host",
        "value": "pwa-open-graph-next.vercel.app"
        }
      ]
    }
  ]
}